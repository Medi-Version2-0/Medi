    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "start2": "electron-forge start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
